{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@styles/*": ["./src/styles/*"],
      "@consts/*": ["./src/consts/*"],
      "@redux/*": ["./src/redux/*"],
      "@icons/*": ["./src/icons/*"],
      "@img/*": ["./src/img/*"],
      "@contextProviders/*": ["./src/contextProviders/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@clients/*": ["./src/clients/*"],
      "@customTypes/*": ["./src/customTypes/*"],
      "@generic/*": ["./src/components/generic/*"],
      "@homepage/*": ["./src/components/homepage/*"],
      "@productsPage/*": ["./src/components/productsPage/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
